[user]
  name = Jacob Strieb
  email = jstrieb@users.noreply.github.com
[color]
	ui = auto
[alias]
  adog = log --all --decorate --oneline --graph
  difflog = log --all --decorate -c
[checkout]
  defaultRemote = origin
[init]
  defaultBranch = master
[pull]
	rebase = false
[commit]
  verbose = true
[branch]
  sort = committerdate
